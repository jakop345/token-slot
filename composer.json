{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "5.2.*",
		"tokenly/xchain-client": "^0.2",
		"tokenly/bitcoin-currency-lib": "^1",
		"tokenly/hmac-auth": "^1",
		"linusu/bitcoin-address-validator": "~0",
		"guzzlehttp/guzzle": "~6.0",
		"satooshi/php-coveralls": "~0",
		"tokenly/token-generator": "dev-master@dev",
        "tokenly/laravel-api-provider": "^0.1",
        "tokenly/laravel-event-log": "dev-master@dev",
		"tokenly/laravel-blocking-beanstalkd": "dev-master@dev",
		"tokenly/xcaller-client": "dev-master@dev",
		"pda/pheanstalk": "~3",
		"laravelcollective/bus": "dev-master@dev"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1"
	},
	"autoload": {
		"classmap": [
			"database",
			"app/Models",
			"app/Libraries"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
